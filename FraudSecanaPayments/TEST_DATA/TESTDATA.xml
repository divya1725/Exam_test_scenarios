  <con:testStep type="jdbc" name="BM_V_AUTH_USERINFO" id="0dd16c75-a6f6-4788-87e6-c56fcad4ca2d">
    <con:settings>
      <con:setting id="prettyPrintResponse">true</con:setting>
    </con:settings>
    <con:config xsi:type="con:JdbcRequestTestStep" convertColumnNamesToUpperCase="true" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
      <con:dbConnectionName>PWHDATA</con:dbConnectionName>
      <con:driver>${#Project#JDBCDriver}</con:driver>
      <con:connectionString>${#Project#PWHDbString}</con:connectionString>
      <con:password/>
      <con:connectionProperties/>
      <con:query>SELECT V.AGREEMENTID,A.CUSTOMERID,V.USERID,
V.ACCOUNTNUMBER,V.CURRENCYCODE
FROM V_AUTH_USERINFO V
LEFT OUTER JOIN AUTH_USERAGREEMENT A
ON V.AGREEMENTID = A.AGREEMENTID
WHERE V.BANKORGID = '${BM_Properties#orgid}'
AND V.MARKETTYPE='BM' AND V.CHANNEL='BES'
